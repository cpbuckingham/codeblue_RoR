

		<!-- Page Wrapper -->
			<div id="page-wrapper">

<br><br>
<div class="wrapper">
  <div class="inner">
		<p id="notice"><%= notice %></p>

    <div class="table-wrapper">
    <table class="alt">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Github username</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coders.each do |coder| %>
      <tr>
        <td><%= coder.name %></td>
        <td><%= coder.email %></td>
        <td><%= coder.github_username %></td>
        <td><%= link_to 'Show', coder %><br><%= link_to 'Edit', edit_coder_path(coder) %></td>
        <td>
					<!-- <%= link_to "Gather PR", class:"button small fit" %><br><%= link_to "Reward", class:"button special small fit" %> -->
					<%= form_for coder, :method => :delete do |f| %><%= f.submit "Delete" %><% end %>
				</td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to '+  New Coder', new_coder_path %>

</div>
</div>
</div>


